{{!
string search-title

}}
<form action="/Special:Search" method="get" class="form-inline my-2 my-lg-0">
	<input id="searchInput" class="form-control mr-sm-2" type="search" name="search" title="{{ search-title }} [⌃⌥f]" placeholder="{{ search-title }}" aria-label="{{ search-title }}" accesskey="f" autocomplete="on">
	<span class="input-group-append">
		<button type="submit" class="btn btn-outline-primary my-2 my-sm-0" type="submit"><i class="fa fa-search "></i></button>
	</span>
</form>
